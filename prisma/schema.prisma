// database
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// generator
generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 4
}

model User {
  id               Int     @id @default(autoincrement())
  binance_token    String  @unique
  binance_secret   String  @unique
  username         String  @unique
  password         String
  money            String
}
