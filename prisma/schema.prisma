// database
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// generator
generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

model User {
  id            Int     @id @default(autoincrement())
  binance_token String  @unique
  username      String  @unique
  money         String  @unique
}
